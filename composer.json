{
    "name": "laz/lara-vue-translate",
    "description": "Use Laravel Translation in Vue",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lt\\LaraVueTranslate\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Temitope Olotin",
            "email": "olotintemitope@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Lt\\LaraVueTranslate\\Providers\\TranslationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    }
}
